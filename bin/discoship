#!/usr/bin/env python3
import logging
import sys

from discoship.cli import DiscoShipArgParser, delegate_args

args = DiscoShipArgParser.parse_args()
print(args, file=sys.stderr)
if args.debug:
    logging.basicConfig(level=logging.DEBUG)
else:
    logging.basicConfig(level=logging.INFO)
delegate_args(args)
